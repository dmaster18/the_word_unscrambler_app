    )\r\n    }\r\n  }\r\n\r\n  increment = (userInput) => {\r\n    let newScore = this.state.score + userInput.length\r\n    this.setState({\r\n     score: newScore\r\n    })\r\n }\r\n\r\n  render() {\r\n    return (\r\n      <div id=\"game\">\r\n      {this.state.wordSession.name &&\r\n        <WordContainer letterArray={this.shuffle(this.state.wordSession.name)} name={this.state.wordSession.name} allWords={this.state.wordSession.all_words} score ={this.state.score} increment={this.increment}/>\r\n      }\r\n        <button onClick={this.next}>{this.state.nextText}</button>\r\n      </div>\r\n    )\r\n  }\r\n}\r\n","C:\\Users\\user\\Google Drive\\Flatiron School Boot Camp\\Portfolio Projects\\Portfolio Project #5\\the_word_unscrambler\\frontend\\src\\game-components\\Word.js",[],"C:\\Users\\user\\Google Drive\\Flatiron School Boot Camp\\Portfolio Projects\\Portfolio Project #5\\the_word_unscrambler\\frontend\\src\\game-components\\Response.js",["51"],"import React, { Component } from 'react';\r\n\r\nexport default class Response extends Component {\r\n\r\n\r\n\r\n  render() {\r\n        return null\r\n  }\r\n}\r\n","C:\\Users\\user\\Google Drive\\Flatiron School Boot Camp\\Portfolio Projects\\Portfolio Project #5\\the_word_unscrambler\\frontend\\src\\game-components\\Timer.js",["52"],"import React, { Component } from 'react';\r\n\r\nexport default class Timer extends Component {\r\n\r\n  render() {\r\n        return null\r\n  }\r\n}\r\n","C:\\Users\\user\\Google Drive\\Flatiron School Boot Camp\\Portfolio Projects\\Portfolio Project #5\\the_word_unscrambler\\frontend\\src\\game-components\\LeaderBoard.js",["53"],"import React, { Component } from 'react';\r\n\r\nexport default class LeaderBoard extends Component {\r\n\r\n  render() {\r\n    return null\r\n  }\r\n}\r\n","C:\\Users\\user\\Google Drive\\Flatiron School Boot Camp\\Portfolio Projects\\Portfolio Project #5\\the_word_unscrambler\\frontend\\src\\game-components\\WordContainer.js",[],{"ruleId":"54","replaced